region        = "eu-west-1"
aws_profile   = "default"
cluster_name  = "my-eks-cluster"
cluster_version = "1.28"

node_groups = {
  managed_nodes = {
    desired_capacity = 2
    min_capacity     = 1
    max_capacity     = 3
    instance_types   = ["t3.small"]
    key_name         = "my-key"
    additional_tags  = {}
  }
}

tags = {
  Owner       = "dev-team"
  Environment = "dev"
  Project     = "demo-eks"
}